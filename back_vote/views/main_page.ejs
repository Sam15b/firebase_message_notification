            .current-elections a:nth-child(2),
            .previous-elections a:nth-child(2) {
                  background-color: #f2e8df;
            }

            .current-elections a:nth-child(3),
            .previous-elections a:nth-child(3) {
                  background-color: #e0f7fa;
            }

            .current-elections a:nth-child(4),
            .previous-elections a:nth-child(4) {
                  background-color: #e0f4e8;
            }

            .current-elections a:nth-child(5),
            .previous-elections a:nth-child(5) {
                  background-color: #f2e0f4;
            }

            .current-elections a:nth-child(6),
            .previous-elections a:nth-child(6) {
                  background-color: #e0eaf2;
            }

            .elections a:hover {
                  background-color: #ccc;
